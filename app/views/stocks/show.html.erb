        <%-# Section 3 -%>
        <section class="about-valuation-section">
          <header class="header-section">
            <div class="container-title-section">
              <div class="title-section"><h2 class="title-content-h2"><span class="title-content-span">About <%= @stock.name %></span></h2></div>
            </div>
          </header>
          <div class="about-container">
            <% if @stock.earning %>
              <div class="extend-about" style="display:block;">
                <h3 class="about-h3"><span class="about-span"><%= @stock.earning[0..245] %>
                <span class="about-link"><span class="about-link-span" id="read-more">... Read More</span></span>
                </span></h3>
              </div>
              <div class="reduce-about" style="display:none;">
                <h3 class="about-h3"><span class="about-span"><%= @stock.earning %>
                <span class="about-link"><span class="about-link-span" id="read-less">Read Less</span></span>
                </span></h3>
              </div>
            <% end %>
            <div class="grid-valuation">
              <div class="grid-element-container">
                <div class="col-mg-val-1">
                  <div class="name-data">CEO</div>
                  <div class="value-data"><%= @stock.ceo %></div>
                </div>
                <div class="col-mg-val-2">
                  <div class="name-data">Employees</div>
                  <div class="value-data"><%= @stock.revenue.split("Employees").last %></div>
                </div>
                <div class="col-mg-val-3">
                  <div class="name-data">Headquarters</div>
                  <div class="value-data">Dearborn, Michigan</div>
                </div>
                <div class="col-mg-val-4">
                  <div class="name-data">Founded</div>
                  <div class="value-data"><%= @stock.founded %></div>
                </div>
                <div class="col-mg-val-5">
                  <div class="name-data">Market Cap</div>
                  <div class="value-data"><%= @stock.ebitda.split("Market cap").last.split("EPS").first unless @stock.ebitda.nil?%></div>
                </div>
                <div class="col-mg-val-6">
                  <div class="name-data">Price-Earnings Ratio</div>
                  <div class="value-data"><%= @stock.ebitda.split("P/E (TTM)").last.split("Market cap").first unless @stock.ebitda.nil?%></div>
                </div>
                <div class="col-mg-val-7">
                  <div class="name-data">Dividend Yield</div>
                  <div class="value-data">9,01</div>
                </div>
                <div class="col-mg-val-8">
                  <div class="name-data">Average volume</div>
                  <div class="value-data"><%= @stock.ebitda.split("volume").last.split("Shares outstanding").first unless @stock.ebitda.nil?%></div>
                </div>
                <div class="col-mg-val-9">
                  <div class="name-data">Earnings per share</div>
                  <div class="value-data"><%= @stock.ebitda.split("EPS (TTM)").last.split("Annual").first[0..5] unless @stock.ebitda.nil? %></div>
                </div>
                <div class="col-mg-val-10">
                  <div class="name-data">Low / High Today</div>
                  <div class="value-data"><%= @stock.ebitda.split("Low").last.split("Bid").first unless @stock.ebitda.nil? %> / <%= @stock.ebitda.split("High").last.split("Low").first unless @stock.ebitda.nil? %></div>
                </div>
                <div class="col-mg-val-11">
                  <div class="name-data">Open Price</div>
                  <div class="value-data"><%= @stock.ebitda.split("Open").last.split("High").first unless @stock.ebitda.nil? %></div>
                </div>
                <div class="col-mg-val-12">
                  <div class="name-data">Free float</div>
                  <div class="value-data"><%= @stock.ebitda.split("Free float").last.split("P/E (TTM)").first unless @stock.ebitda.nil? %></div>
                </div>
                <div class="col-mg-val-13">
                  <div class="name-data">52 Week High</div>
                  <div class="value-data">10,35 $US</div>
                </div>
                <div class="col-mg-val-14">
                  <div class="name-data">52 Week Low</div>
                  <div class="value-data">3,96 $US</div>
                </div>
                <div class="display-mg">
                  <div class="btn-also-like">
                    <button class="btn-ctn display-mg"><span class="also-s"><span class="to-change-ct">Display</span> Valuation</span></button>
                  </div>
                </div>
              </div>
            </div>
            <div class="grid-morningstar-valuation" style="display:none;">
              <% @stock.fundamentals.each do |fundamental| %>
                <div class="col-mg-val-1">
                  <div class="name-data">Price / Sales</div>
                  <div class="value-data"><%= fundamental.current_price_sales %></div>
                </div>
                <div class="col-mg-val-2">
                  <div class="name-data">Price / Earnings</div>
                  <div class="value-data"><%= fundamental.current_price_earnings %></div>
                </div>
                <div class="col-mg-val-3">
                  <div class="name-data">Price / Book</div>
                  <div class="value-data"><%= fundamental.current_price_book %></div>
                </div>
                <div class="col-mg-val-4">
                  <div class="name-data">Price / Forward Earnings</div>
                  <div class="value-data"><%= fundamental.current_price_forward_earnings %></div>
                </div>
                <div class="col-mg-val-5">
                  <div class="name-data">Price / Cash Flow</div>
                  <div class="value-data"><%= fundamental.current_price_cashflow %></div>
                </div>
                <div class="col-mg-val-6">
                  <div class="name-data">Earnings Yield</div>
                  <div class="value-data"><%= fundamental.current_earning_yield %></div>
                </div>
                <div class="col-mg-val-7">
                  <div class="name-data">Enterprise Value (Bil or Mil)</div>
                  <div class="value-data"><%= fundamental.current_enterprise_value %></div>
                </div>
                <div class="col-mg-val-8">
                  <div class="name-data">Enterprise Value / Ebit</div>
                  <div class="value-data"><%= fundamental.current_enterprise_value_ebit %></div>
                </div>
                <div class="col-mg-val-9">
                  <div class="name-data">Enterprise Value / Ebitda</div>
                  <div class="value-data"><%=fundamental.current_enterprise_value_ebitda  %></div>
                </div>
              <% end %>
            </div>
          </div>
        </section>

        <%-# Section 4 -%>
        <% if @stock.tag_counts.count > 0 %>
          <section class="collection-tags-section">
            <header class="header-section">
              <div class="container-title-section">
                <div class="title-section">
                  <h2 class="title-content-h2"><span class="title-content-span">Collections</title></h2>
                </div>
              </div>
            </header>
            <div class="tags-div-container">
              <% @stock.tag_list.each do  |tag| %>
