<% if current_user.admin %>
  <div class="stock-show-admin">
    <a href="/stocks/<%= @stock.id %>/comments/new"><button class="button type2">New Comment</button></a>
  </div>
<% end %>

<div class="stock-show-container">
  <main class="main-container">
    <% if @stock.tag_counts.count > 0  %>
      <div class="top-tags-container">
        <div class="tags-btn-robinhood">
          <a class="a-tags-link a-tags-inherit">
            <span class="btn-tags-span"><%= @stock.tag_list[0] %></span>
          </a>
        </div>
        <div class="tags-btn-robinhood">
          <a class="a-tags-link a-tags-inherit" rel href="#">
            <span class="btn-tags-span"><%= @stock.tag_list[1] %></span>
           </a>
        </div>
        <div class="tags-btn-robinhood">
          <a class="a-tags-link a-tags-inherit" rel href="#">
            <span class="btn-tags-span"><%= @stock.tag_list[2] %></span>
           </a>
        </div>
      </div>
    <% end %>
  </main>
  <div class="show-row">
    <div class="show-content-col-12">
      <header class="show-header">
        <h1 class="stock-show-name"><%= @stock.name %></h1>
      </header>
      <div>
        <%-# Section 1 -%>
          <section class="container-chart">
            <header class="header-chart">

            <%-# Header Graph div 1 -%>
              <div class="h-1">
                <h1 class="h-1-1">
                  <span class="h-1-1-s">
                    <span class="up">
                      <div class="h-1-1-s-u-d"><%= @stock.price %> <%= @stock.currency %></div>
                    </span>
                  </span>
                </h1>
              </div>

              <%-# Header Graph span 2, 2.1 et 2.2 -%>
              <div class="h-2">
                <span class="h-2-s1">
                  <span>
                    <span>-<%= @stock.price %> <%= @stock.currency %></span>
                  </span>
                  <span>
                    <span><%= @stock.price_yrchg %></span>
                  </span>
                </span>
                <span class="h-2-s2">Past Year.  as of <%= @stock.ebitda.split("Data delayed at least 15 minutes, as of").last.split(".More â–¼").first unless @stock.ebitda.nil? %></span>
              </div>
            </header>

            <%-# Graph section -%>
            <div>
              <div class="graph-container">
                <div class="graph-container-size">
                </div>
              </div>
            </div>

            <%-# Nav section -%>
            <nav class="nav-graph" bounds="regular">
              <div class="nav-list">
                <button type="button" class="btn-nav-robinhood">
                  <span class="btn-nav-robinhood-span-1">
                    <span class="btn-nav-robinhood-pb">1D</span>
                  </span>
                </button>
                <button type="button" class="btn-nav-robinhood">
                  <span class="btn-nav-robinhood-span-1">
                    <span class="btn-nav-robinhood-pb">1W</span>
                  </span>
                </button>
                <button type="button" class="btn-nav-robinhood">
                  <span class="btn-nav-robinhood-span-1">
                    <span class="btn-nav-robinhood-pb">1M</span>
                  </span>
                </button>
                <button type="button" class="btn-nav-robinhood">
                  <span class="btn-nav-robinhood-span-1">
                    <span class="btn-nav-robinhood-pb">3M</span>
                  </span>
                </button>
                <button type="button" class="btn-nav-robinhood">
                  <span class="btn-nav-robinhood-span-1">
                    <span class="btn-nav-robinhood-pb">1Y</span>
                  </span>
                </button>
                <button type="button" class="btn-nav-robinhood">
                  <span class="btn-nav-robinhood-span-1">
                    <span class="btn-nav-robinhood-pb">5Y</span>
                  </span>
                </button>
              </div>
            </nav>
          </section>

        <%-# Section 2 -%>
        <section class="advertising-section">
          <div class="advertising-container">
