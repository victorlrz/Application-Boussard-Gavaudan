<h1>Create a new Acquirer</h1>

<div class="container">
  <%= simple_form_for @acquirer do |f| %>
    <%= f.input :name %>
    <%= f.input :sector %>
    <%= f.input :identifier %>
    <%= f.input :url %>
    <%= f.input :acquirer_logo, as: :file, input_html: { class: "btn btn-outline-primary" } %>
    <%= f.submit "Create", class: "btn btn-primary" %>
  <% end %>
</div>



