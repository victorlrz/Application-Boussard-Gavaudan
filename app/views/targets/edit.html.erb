<h1>Edit the Target</h1>

<div class="container">
  <%= simple_form_for @target do |f| %>
    <%= f.input :name %>
    <%= f.input :sector %>
    <%= f.input :description %>
    <%= f.input :ceo %>
    <%= f.input :annual_report, as: :file, input_html: { class: "btn btn-outline-primary" } %>
    <%= f.input :target_logo, as: :file, input_html: { class: "btn btn-outline-primary" } %>
    <%= f.submit "Create", class: "btn btn-primary" %>
  <% end %>
</div>



